<?xml version="1.0" encoding="UTF-8"?>
<data 
	xmlns="https://www.woltlab.com"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/6.0/userGroupOption.xsd"
>
	<import>
		<categories>
			<category name="user.minecraft.dynmap">
				<parent>user.minecraft</parent>
			</category>
			<category name="mod.minecraft.dynmap">
				<parent>mod.minecraft</parent>
			</category>
			<category name="admin.minecraft.dynmap">
				<parent>admin.minecraft</parent>
			</category>
		</categories>
		<options>
			<option name="user.minecraft.dynmap.canUseWebchat">
				<categoryname>user.minecraft.dynmap</categoryname>
				<optiontype>boolean</optiontype>
				<usersonly>0</usersonly>
				<defaultvalue>0</defaultvalue>
				<userdefaultvalue>1</userdefaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="mod.minecraft.dynmap.canSeeAllPlayers">
				<categoryname>mod.minecraft.dynmap</categoryname>
				<optiontype>boolean</optiontype>
				<usersonly>1</usersonly>
				<userdefaultvalue>0</userdefaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<admindefaultvalue>1</admindefaultvalue>
			</option>
			<option name="user.minecraft.dynmap.noAccess">
				<categoryname>user.minecraft.dynmap</categoryname>
				<optiontype>DynmapAccess</optiontype>
				<usersonly>0</usersonly>
				<defaultvalue></defaultvalue>
			</option>
			<option name="user.profile.canHideDynmap">
				<categoryname>user.minecraft.dynmap</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
		</options>
	</import>
</data>
<?xml version="1.0" encoding="UTF-8"?>
<data
	xmlns="https://www.woltlab.com"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://www.woltlab.com https://www.woltlab.com/XSD/6.0/option.xsd"
>
	<import>
		<categories>
			<category name="dynmap_general">
				<parent>minecraft</parent>
			</category>
		</categories>
		<options>
			<option name="dynmap_general_host">
				<categoryname>dynmap_general</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>localhost</defaultvalue>
			</option>
			<option name="dynmap_general_port">
				<categoryname>dynmap_general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>3306</defaultvalue>
			</option>
			<option name="dynmap_general_name">
				<categoryname>dynmap_general</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>dynmap</defaultvalue>
			</option>
			<option name="dynmap_general_user">
				<categoryname>dynmap_general</categoryname>
				<optiontype>Text</optiontype>
			</option>
			<option name="dynmap_general_password">
				<categoryname>dynmap_general</categoryname>
				<optiontype>Password</optiontype>
			</option>
			<option name="dynmap_general_webchat_enabled">
				<categoryname>dynmap_general</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
			</option>
			<option name="dynmap_general_webchat_interval">
				<categoryname>dynmap_general</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>5</defaultvalue>
			</option>
		</options>
	</import>
</data>
